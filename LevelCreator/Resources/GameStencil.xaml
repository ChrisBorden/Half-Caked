<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:LevelCreator">
    <s:Toolbox x:Key="GameStencil">
        <ItemsControl.Items>
            <Image IsHitTestVisible="False"
                   Stretch="Uniform"
                   Source="../Images/Checkpoint.png" 
                   ToolTip="Checkpoint"/>
            <Image IsHitTestVisible="False"
                   Stretch="Uniform"
                   Source="../Images/Door.png" 
                   ToolTip="Door"/>
            <Image IsHitTestVisible="False"
                   Stretch="Uniform"
                   Source="../Images/Enemy.png" 
                   ToolTip="Enemy"/>
            <Image IsHitTestVisible="False"
                   Stretch="Uniform"
                   Source="../Images/Platform.png" 
                   ToolTip="Platform"/>
            <Image IsHitTestVisible="False"
                   Stretch="Uniform"
                   Source="../Images/Switch.png" 
                   ToolTip="Switch"/>
            <Image IsHitTestVisible="False"
                   Stretch="Fill"
                   Source="../Images/Tile.png" 
                   ToolTip="Tile"/>
        </ItemsControl.Items>
    </s:Toolbox>
</ResourceDictionary>